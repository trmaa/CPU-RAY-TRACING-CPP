{
    "name": "Example Warm and Legible Color Scheme",
    "globals":
    {
        "background": "rgb(24, 24, 34)",
        "foreground": "#ec7",
        "caret": "white",
        "font_family": "consolas"
    },
    "rules":
    [
        {
            "name": "Comment",
            "scope": "comment",
            "foreground": "#88f"
        },
        {
            "name": "String",
            "scope": "string",
            "foreground": "#af3"
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "foreground": "#fa3"
        },
        {
            "name": "Number Suffix",
            "scope": "storage.type.numeric",
            "foreground": "#fa3"
        },
        {
            "name": "Function",
            "scope": "entity.name.function",
            "foreground": "#f20",
            "font_style": "bold"
        },
        {
            "name": "Function call",
            "scope": "variable.function, variable.annotation",
            "foreground": "#f02",
            "font_style": "bold"
        },
        {
            "name": "Control Keywords",
            "scope": "keyword.control",
            "foreground": "#f05",
            "font_style": "bold"
        },
        {
            "name": "Other Keywords",
            "scope": "keyword.other",
            "foreground": "#b77",
            "font_style": "bold"
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "foreground": "#fba",
            "font_style":"bold"
        },
        {
            "name": "Storage",
            "scope": "storage.type",
            "foreground": "#f70",
            "font_style": "bold"
        },
        {
            "name": "Support",
            "scope": "support.type",
            "foreground": "#f75",
            "font_style": "bold"
        },
        {
            "name": "Punctuation",
            "scope": "punctuation.definition - punctuation.definition.numeric.base",
            "foreground": "var(blue5)"
        },
        {
            "name": "Library function",
            "scope": "support.function, support.macro",
            "foreground": "#f70",
            "font_style": "bold italic"
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "foreground": "#f70",
            "font_style": "bold italic"
        },
        {
            "name": "Library class/type",
            "scope": "support.type, support.class",
            "foreground": "#f70",
            "font_style": "bold italic"
        },
        {
            "name": "Brackets",
            "scope": "punctuation.section",
            "foreground": "#fff"
        },
        {
            "name": "Selection",
            "scope": "meta.selection",
            "background": "#777"
        },
        {
            "name": "PublicPrivateStatic",
            "scope": "storage.modifier.access.cs, storage.modifier.static.cs",
            "foreground": "#f77"
        }
    ],
    "variables":
    {
        "black": "hsl(0, 0%, 0%)",
        "blue": "hsl(210, 50%, 60%)",
        "blue-vibrant": "hsl(210, 60%, 60%)",
        "blue2": "hsla(210, 13%, 40%, 0.7)",
        "blue3": "hsl(210, 15%, 22%)",
        "blue4": "hsl(210, 13%, 45%)",
        "blue5": "hsl(180, 36%, 54%)",
        "blue6": "hsl(221, 12%, 69%)",
        "green": "hsl(114, 31%, 68%)",
        "grey": "hsl(0, 0%, 20%)",
        "orange": "hsl(32, 93%, 66%)",
        "orange2": "hsl(32, 85%, 55%)",
        "orange3": "hsl(40, 94%, 68%)",
        "pink": "hsl(300, 30%, 68%)",
        "red": "hsl(357, 79%, 65%)",
        "red2": "hsl(13, 93%, 66%)",
        "white": "hsl(0, 0%, 100%)",
        "white2": "hsl(0, 0%, 97%)",
        "white3": "hsl(219, 28%, 88%)"
    }
}